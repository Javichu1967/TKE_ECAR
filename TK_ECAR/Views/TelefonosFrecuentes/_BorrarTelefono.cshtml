@using TK_ECAR.Models
@model TelefonosFrecuentesModels

@Html.HiddenFor(m => m.Accion)
<div class="form-horizontal">
    <div class="form-group">       
        @Html.LabelFor(m => m.NUMERO_TELEFONO, new { @class = "control-label col-sm-3" })
        <div class="col-sm-8">
            @Html.TextBoxFor(m => m.NUMERO_TELEFONO, new { @class = "form-control", @readonly = true })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(m => m.DESCRIPCION, new { @class = "control-label col-sm-3" })
        <div class="col-sm-8">
            @Html.TextBoxFor(m => m.DESCRIPCION, new { @class = "form-control", @readonly = true })
        </div>
    </div>
</div>

 